{
  "_version": "1.58.0",
  "sap.app": {
    "id": "com.segezha.form.mpr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "Плитка МПР для создания заявки на получение полномочий",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "odata": {
        "uri": "/sap/opu/odata/sap/ZBC_REQUEST_ROLE_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "defaultCountMode": "InlineRepeat",
          "useBatch": true
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "com.segezha.form.mpr.view.App",
      "type": "XML",
      "async": true
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "path": "com.segezha.form.mpr.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "type": "View",
        "viewType": "XML",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": "home"
        }
      ],
      "targets": {
        "home": {
          "name": "Home"
        }
      }
    },
    "dependencies": {
      "minUI5Version": "1.74.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "cozy": true,
      "compact": false
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.segezha.form.mpr.i18n.i18n"
        }
      },
      "": {
        "dataSource": "odata",
        "preload": true,
        "settings": {
          "odataVersion": "2.0",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "InlineRepeat",
          "useBatch": true,
          "headers": {
            "Accept": "application/atom+xml,application/atomsvc+xml,application/json"
          }
        }
      },
      "Request_data": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "RRequest": "0",
          "Status": "черновик",
          "Login": "testLogin",
          "NewUser": false,
          "Bname": "textBname",
          "McNamelas": "testMcNamelas",
          "McNamefir": "testMcNamefir",
          "DataFrom": "",
          "DataTo": "",
          "Namemiddle": "testNamemiddle",
          "Department": "testDepartment",
          "Function": "testFunction",
          "SmtpAddr": "testSmtpAddr@segezha",
          "Leader": "testLeader",
          "UserCopy": "testUserCopy",
          "Bukrs": "1000",
          "Werks": "1111",
          "Statu": "Тестовая заявка",
          "LoginName": "testLoginName",
          "LeaderName": "testLeaderName",
          "UserCopyName": "testUserCopyName",
          "Butxt": "test Butxt",
          "Name1": "test Name1",
          "CuserName": "testCuserName",
          "UuserName": "testUuserName",
          "LuserName": "testLuserName",
          "VuserName": "testVuserName",
          "Active": true,
          "LeaderExt": false,
          "CommentExt": "testCommentExt",
          "SoglasForm": {},
          "Guid": []
        }
      },
      "VHbukrs": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "items": []
        }
      },
      "VHWerks": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "items": []
        }
      },
      "VHuser": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "items": []
        }
      },
      "VHRoleName": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "items": []
        }
      },
      "VHRoleTransaction": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
