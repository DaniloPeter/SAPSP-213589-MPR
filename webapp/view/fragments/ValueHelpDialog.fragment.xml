<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
        id="genericValueHelpDialog"
        title="{title}"
        supportMultiselect="false"
        ok=".onGenericOkPress"
        cancel=".onGenericCancelPress"
        supportRanges="false">
        <filterBar>
            <fb:FilterBar id="genericFilterBar" advancedMode="true" search=".onGenericSearch">
                <!-- FilterGroupItems -->
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>