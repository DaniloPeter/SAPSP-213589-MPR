<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
        title="Справочник Incoterms"
        supportMultiselect="false"
        ok=".onValueHelpOkPress"
        cancel=".onValueHelpCancelPress"
        supportRanges="false"
        key="ID"
        descriptionKey="Text">
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="ID"
                        label="Код"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="ID" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Text"
                        label="Описание"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Text" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>