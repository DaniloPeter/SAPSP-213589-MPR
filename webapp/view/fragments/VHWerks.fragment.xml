<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
        title="Справочник заводов"
        supportMultiselect="false"
        ok=".onWerksOkPress"
        cancel=".onWerksCancelPress"
        supportRanges="false"
        key="Werks"
        descriptionKey="Name1">
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onFilterBarSearchWerks">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="Werks"
                        name="Bukrs"
                        label="Организация (BU)"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Bukrs" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="Werks"
                        name="Werks"
                        label="Код завода"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Werks" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="Werks"
                        name="Name1"
                        label="Наименование"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Name1" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="Werks"
                        name="Ort01"
                        label="Город"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Ort01" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="Werks"
                        name="Land1"
                        label="Страна"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Land1" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>